{
  "name": "@qco/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    }
  },
  "main": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "test": "export NODE_OPTIONS='--max-old-space-size=16096' && bun with-env vitest --logLevel=info",
    "test:run": "export NODE_OPTIONS='--max-old-space-size=16096' && bun with-env vitest run",
    "test:coverage": "export NODE_OPTIONS='--max-old-space-size=16096' && bun with-env vitest run --coverage",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@qco/auth": "workspace:*",
    "@qco/db": "workspace:*",
    "@qco/validators": "workspace:*",
    "@sindresorhus/slugify": "2.2.1",
    "@trpc/server": "11.4.4",
    "date-fns": "4.1.0",
    "nanoid": "^5.1.5",
    "superjson": "2.2.2",
    "zod": "4.0.16"
  },
  "devDependencies": {
    "@qco/eslint-config": "workspace:*",
    "@qco/prettier-config": "workspace:*",
    "@qco/tests": "workspace:*",
    "@qco/tsconfig": "workspace:*",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "tsup": "8.5.0",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "prettier": "@qco/prettier-config"
}
